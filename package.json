{
  "name": "mokuji.js",
  "version": "1.4.2",
  "description": "A table of content JavaScript Library",
  "main": "lib/index.js",
  "scripts": {
    "build": "babel src --out-dir lib && npm run build:umd && npm run build:umd:min",
    "build:umd": "webpack src/index.js dist/mokuji.js",
    "build:umd:min": "cross-env NODE_ENV=production webpack --mode production src/index.js dist/mokuji.min.js",
    "clean": "rimraf lib dist coverage",
    "lint": "eslint src",
    "format": "eslint --fix src",
    "test": "npm run lint",
    "prepublish": "npm run build"
  },
  "lint-staged": {
    "src/*.js": [
      "eslint --fix",
      "eslint",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hiro0218/mokuji.js.git"
  },
  "keywords": [
    "table of content",
    "toc",
    "contents",
    "index"
  ],
  "author": "hiro",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hiro0218/mokuji.js/issues"
  },
  "homepage": "https://github.com/hiro0218/mokuji.js",
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "7.1.5",
    "@babel/core": "7.1.6",
    "@babel/plugin-transform-for-of": "7.0.0",
    "@babel/plugin-transform-object-assign": "7.0.0",
    "@babel/preset-env": "7.1.6",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "cross-env": "^5.2.0",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-promise": "^3.7.0",
    "husky": "^1.1.4",
    "lint-staged": "^8.0.4",
    "prettier": "^1.15.2",
    "rimraf": "^2.6.2",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
