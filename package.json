{
  "name": "mokuji.js",
  "version": "4.3.0",
  "description": "A table of content JavaScript Library",
  "scripts": {
    "build": "tsup ./src",
    "dev": "tsup ./src --watch",
    "lint": "eslint ./src/**/*.ts",
    "prepare": "npm run build && husky install"
  },
  "source": "src/index.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "directories": {
    "lib": "lib"
  },
  "files": [
    "dist",
    "LICENSE",
    "package.json",
    "READNME.md"
  ],
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix --cache",
      "eslint --cache",
      "prettier --write --cache"
    ],
    "*.{ts}": [
      "bash -c 'tsc --noEmit --skipLibCheck'"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hiro0218/mokuji.js.git"
  },
  "keywords": [
    "table of content",
    "toc",
    "contents",
    "index"
  ],
  "author": "hiro",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hiro0218/mokuji.js/issues"
  },
  "homepage": "https://github.com/hiro0218/mokuji.js",
  "devDependencies": {
    "@types/node": "~20.2.3",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-unicorn": "^47.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.2",
    "prettier": "^2.8.4",
    "rimraf": "^4.1.2",
    "tsup": "^6.7.0",
    "typescript": "~5.0.4"
  }
}
