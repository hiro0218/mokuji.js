{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "browsers": [
            "ie 11",
            "safari >= 10"
          ]
        },
        "modules": false
      }
    ]
  ],
  "plugins": [
    [
      "@babel/plugin-transform-for-of",
      {
        "loose": true
      }
    ],
    [
      "@babel/plugin-transform-object-assign",
      {
        "loose": true
      }
    ]
  ]
}
